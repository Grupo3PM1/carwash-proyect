        tools:layout="@layout/fragment_cambios_aceite" />

    <fragment
        android:id="@+id/nav_lavados"
        android:name="com.aplicacion.elcatrachocarwash.ui.historial.LavadosFragment"
        android:label="Lavados Realizados"
        tools:layout="@layout/fragment_lavados" />

    <fragment
        android:id="@+id/nav_cliente"
        android:name="com.aplicacion.elcatrachocarwash.ui.configuracionPerfil.PerfilUsuarioFragment"
        android:label="Mi Perfil"
        tools:layout="@layout/fragment_perfil_usuario" />

    <fragment
        android:id="@+id/nav_registrar_auto"
        android:name="com.aplicacion.elcatrachocarwash.ui.configuracionPerfil.PerfilVehiculoFragment"
        android:label="Registrar Vehiculo"
        tools:layout="@layout/fragment_perfil_vehiculo" />





</navigation>