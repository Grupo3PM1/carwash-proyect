<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_principal">

    <fragment
        android:id="@+id/nav_principal"
        android:name="com.aplicacion.elcatrachocarwash.ui.principal.PrincipalFragment"
        android:label="CarWash 'El Catracho' "
        tools:layout="@layout/fragment_principal" />

    <fragment
        android:id="@+id/nav_cotizacion"
        android:name="com.aplicacion.elcatrachocarwash.ui.cotizacion.CotizacionFragment"
        android:label="Solicitar Cotizacion"
        tools:layout="@layout/fragment_cotizacion" />

    <fragment
        android:id="@+id/nav_vehiculo"
        android:name="com.aplicacion.elcatrachocarwash.ui.cotizacion.Fragment_Registro_auto"
